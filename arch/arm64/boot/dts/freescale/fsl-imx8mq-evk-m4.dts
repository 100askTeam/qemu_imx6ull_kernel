#include "fsl-imx8mq-evk.dts"

/ {
	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		m4_reserved: m4@0x80000000 {
			no-map;
			reg = <0 0x80000000 0 0x1000000>;
		};

	};

	sound-wm8524 {
		status = "disabled";
	};
};

&gpt1 {
	status = "disabled";
};

&i2c2 {
	status = "disabled";
};

&pwm4 {
	status = "disabled";
};

&tmu {
	status = "disabled";
};

&uart2 {
	status = "disabled";
};

&wdog3{
	status = "disabled";
};

&sai2 {
	status = "disabled";
};

&qspi {
	status = "disabled";
};
